#include<iostream>
#include<cstring> 
 using namespace std;
 int main()
 {double speed1,speed2;
 char a[5];
 char b[5];
 char m[]="m/s";
 char n[]="km/h"; 
cout<<"输入一个速度单位 "<<endl;
cin>>a;
cout<<"输入想要转换成的速度单位 "<<endl;
cin>>b; 
cout<<"输入此单位下速度大小"<<endl;
cin>>speed1;
 if (strcmp(a,m)==0)
 speed2=3.6*speed1; 
 if(strcmp(a,m)==1)
 speed2=speed1/3.6;
 cout<<"换算结果为"<<speed2<<b<<endl; 
  return 0;
 }
